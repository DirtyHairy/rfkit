{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020", "dom"],
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "module": "es2015",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
